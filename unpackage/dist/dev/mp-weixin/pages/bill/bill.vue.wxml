<template name="e0c2c17e">
  <view class="_view data-v-be6bd1e2 container">
    <view class="_view data-v-be6bd1e2 top">
      <image src="{{logo}}" class="_image data-v-be6bd1e2 logo"></image>
      <text class="_text data-v-be6bd1e2 name fs-minor">{{name}}</text>
      <image src="{{wx}}" class="_image data-v-be6bd1e2 code" bindtap="handleProxy" data-eventid="{{'toS-0'}}" data-comkey="{{$k}}"></image>
    </view>
    <view class="_view data-v-be6bd1e2 uni-list fs-head">
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate"> 公司名称：{{company}} </view>
      </view>
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate location">
          <text style="flex-shrink: 0;" class="_text data-v-be6bd1e2">地址：</text>
          <text class="_text data-v-be6bd1e2">{{location}}</text>
        </view>
      </view>
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate" bindtap="handleProxy" data-eventid="{{'zjG-1'}}" data-comkey="{{$k}}"> 电话：{{tel}} </view>
      </view>
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate"> 开户行：{{bank}} </view>
      </view>
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate"> 账号：{{account}} </view>
      </view>
      <view class="_view data-v-be6bd1e2 uni-list-cell">
        <view class="_view data-v-be6bd1e2 uni-list-cell-navigate"> 税号：{{tax}} </view>
      </view>
    </view>
    <view class="_view data-v-be6bd1e2 img-box" wx:if="{{isShow}}" bindtap="handleProxy" data-eventid="{{'SDS-2'}}" data-comkey="{{$k}}">
      <image src="{{wx}}" class="_image data-v-be6bd1e2"></image>
    </view>
  </view>
</template>