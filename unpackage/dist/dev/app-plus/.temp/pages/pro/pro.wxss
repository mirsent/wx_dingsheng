
page {
  color: #666;
}
.type-fixed {
  width: 100%;
  box-sizing: border-box;
  position: fixed;
  z-index: 9999;
  top: 0;
}
.type {
  height: 100rpx;
  padding: 0 30rpx;
  background-color: #F3F3F3;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.type .left,
.type .right {
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.type .left image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10px;
}
.type .right image {
  width: 40rpx;
  height: 30rpx;
  margin-left: 10px;
}
.pciker-text {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.uni-list:after {
  height: 0;
}
.uni-list-cell:after {
  left: 256rpx;
}
.uni-media-list {
  padding: 60rpx 40rpx;
}
.uni-media-list-logo {
  width: 180rpx;
  height: 180rpx;
  margin-right: 36rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);
}
.uni-media-list-body {
  height: auto;
  overflow: auto;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
}
.uni-media-list-text-top {
  margin-bottom: 10px;
}
.uni-media-list-text-bottom {
  line-height: 2em;
}
.none {
  text-align: center;
  padding: 20px;
}