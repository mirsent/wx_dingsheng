<template name="6e5b659f">
  <view class="_view data-v-65e20027 container">
    <view class="_view data-v-65e20027 top">
      <image src="{{logo}}" class="_image data-v-65e20027 logo"></image>
      <text class="_text data-v-65e20027 name fs-minor">{{contact}}</text>
      <image src="{{wx}}" class="_image data-v-65e20027 code" bindtap="handleProxy" data-eventid="{{'zOd-0'}}" data-comkey="{{$k}}"></image>
    </view>
    <view class="_view data-v-65e20027 tel fs-head">
      <view class="_view data-v-65e20027 tel-item">
        <image src="../../static/image/icon_tel.png" class="_image data-v-65e20027"></image>
      </view>
      <view class="_view data-v-65e20027 tel-item">
        <text class="_text data-v-65e20027">联系方式</text>
        <text bindtap="handleProxy" data-eventid="{{'0ls-1'}}" data-comkey="{{$k}}" class="_text data-v-65e20027">{{tel}}</text>
      </view>
      <view class="_view data-v-65e20027 tel-item">
        <text class="_text data-v-65e20027">传真</text>
        <text class="_text data-v-65e20027">{{fax}}</text>
      </view>
    </view>
    <view class="_view data-v-65e20027 map">
      <view class="_view data-v-65e20027 location fs-head"> {{location}} </view>
      <view class="_view data-v-65e20027 img">
        <image src="../../static/image/map.png" mode="widthFix" class="_image data-v-65e20027"></image>
      </view>
    </view>
    <view class="_view data-v-65e20027 img-box" wx:if="{{isShow}}" bindtap="handleProxy" data-eventid="{{'uct-2'}}" data-comkey="{{$k}}">
      <image src="{{wx}}" class="_image data-v-65e20027"></image>
    </view>
  </view>
</template>